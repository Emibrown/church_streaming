                        <hr>
                        <div class="addr clearfix"><em class="fa fa-fw fa-map-marker"></em> <div> <% if(settings.HQaddress) { %> <%= settings.HQaddress %><% } %> <br />
                        </div></div>
                        </address>
            <address>
