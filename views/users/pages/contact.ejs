        <div class="block">
      <div class="contact-page" id="formSubmit">
          <div class="row">
            <div class="col-xs-12 col-sm-6 col-md-4">
            <h3 class="block-title"><span>Contact</span> Us</h3>
            <address>
              <div><em class="fa fa-fw fa-phone"></em> <% if(settings.contactNumber) { %> <%= settings.contactNumber %><% } %></div>
              <div><em class="fa fa-fw fa-fax"></em><% if(settings.prayerNumber) { %> <%= settings.prayerNumber %><% } %> </div>
