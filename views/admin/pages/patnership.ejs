<%- include('../header'); -%>
<%- include('../sidebar'); -%>

<!-- Page Content  -->
<div class="app-main__inner">
   <div class="app-page-title">
       <div class="page-title-wrapper">
           <div class="page-title-heading">
               <div class="page-title-icon">
                   <i class="pe-7s-car icon-gradient bg-mean-fruit"></i>
               </div>
               <div>
                 Patnership
               </div>
           </div> 
        </div>
   </div>        
   <div class="tabs-animation">
    <div class="row">
        <div class="col-md-12">
            <div class="main-card mb-3 card">
                <div class="table-responsive">
                    <table class="align-middle mb-0 table table-borderless table-striped table-hover">
                            <tr>
                                <th class="">Title</th>
                                <td class=""><%= patnership.title %></td>
                            </tr>
                            <tr>
                                <th class="">Sur name</th>
                                <td class=""><%= patnership.surName %></td>
                            </tr>
                            <tr>
                                <th class="">First name</th>
                                <td class=""><%= patnership.firstName %></td>
                            </tr> 
                            <tr>
                                <th class="">Phone no</th>
                                <td class=""><%= patnership.phoneNumber %></td>
                            </tr>
                            <tr>
                                <th class="">Date added</th> 
                                <td class=""><%= moment(patnership.date).format('LL') %></td>
                            </tr>
                            <tr>
                                <th class="">Email</th>
                                <td class=""><%= patnership.email %></td>
                            </tr>
                            <tr>
                                <th class="">membership</th>
                                <td class=""><%= patnership.membership %></td>
                            </tr>
                            <tr>
                                <th class="">fellowship</th>
                                <td class=""><%= patnership.fellowship %></td>
                            </tr>
                            <tr>
                                <th class="">Address</th>
                                <td class=""><%= patnership.address %></td>
                            </tr>
                            <tr>
                                <th class="">whatsapp no</th>
                                <td class=""><%= patnership.whatsAppNo %></td>
                            </tr>
                            <tr>
                                <th class="">Categories</th>
                                <td class=""><%= patnership.categories.join(', ') %></td>
                            </tr>
                            <tr>
                                <th class="">Levels</th>
                                <td class=""><%= patnership.levels.join(', ')  %></td>
                            </tr>
                            <tr>
                                <th class="">Prayer point 1</th>
                                <td class=""><%= patnership.prayerPoint1 %></td>
                            </tr>
                            <tr>
                                <th class="">Prayer point 2</th>
                                <td class=""><%= patnership.prayerPoint2 %></td>
                            </tr>
                     
                    </table>
                </div>
            </div>
        </div>
    </div>
    
  </div>
</div>
<!-- Page Content END -->


<%- include('../footer'); -%>
<%- include('../js'); -%>
<%- include('../../end'); -%>