<%- include('../header'); -%>
<%- include('../sidebar'); -%>

<!-- Page Content  -->
<div class="app-main__inner">
   <div class="app-page-title">
       <div class="page-title-wrapper">
           <div class="page-title-heading">
               <div class="page-title-icon">
                   <i class="pe-7s-car icon-gradient bg-mean-fruit"></i>
               </div>
               <div>
                Music video -  <%= musicVideo.title %>
               </div>
           </div> 
        </div>
   </div>        
   <div class="tabs-animation">
    <div class="row">
        <div class="col-md-12">
            <div class="main-card mb-3 card">
                <div class="table-responsive">
                    <table class="align-middle mb-0 table table-borderless table-striped table-hover">
                            <tr>
                                <th class="">Title</th>
                                <td class=""><%= musicVideo.title %></td>
                            </tr>
                            <tr>
                                <th class="">Genre</th>
                                <td class=""><%= musicVideo.genre %></td>
                            </tr>
                            <tr>
                                <th class="">Supplier name</th>
                                <td class=""><%= musicVideo.supplierName %></td>
                            </tr>
                            <tr>
                                <th class="">Supplier phone no</th>
                                <td class=""><%= musicVideo.phoneNumber %></td>
                            </tr>
                            <tr>
                                <th class="">Date added</th> 
                                <td class=""><%= moment(musicVideo.date).format('LL') %></td>
                            </tr>
                            <tr>
                                <th class="">Supplier email</th>
                                <td class=""><%= musicVideo.email %></td>
                            </tr>
                            <tr>
                                <th class="">Contact name</th>
                                <td class=""><%= musicVideo.contactName %></td>
                            </tr>
                            <tr>
                                <th class="">Location</th>
                                <td class=""><%= musicVideo.location %></td>
                            </tr>
                            <tr>
                                <th class="">Video Url</th>
                                <td class=""><%= musicVideo.videoURL %></td>
                            </tr>
                            <tr>
                                <th class="">Ownership rights</th>
                                <td class=""><%= musicVideo.ownershipRights %></td>
                            </tr>
                            <tr>
                                <th class="">First time production</th>
                                <td class=""><%= musicVideo.firstTimeProduction %></td>
                            </tr>
                            <tr>
                                <th class="">personal relationships</th>
                                <td class=""><%= musicVideo.personalRelationships %></td>
                            </tr>
                     
                    </table>
                </div>
            </div>
        </div>
    </div>
    
  </div>
</div>
<!-- Page Content END -->


<%- include('../footer'); -%>
<%- include('../js'); -%>
<%- include('../../end'); -%>