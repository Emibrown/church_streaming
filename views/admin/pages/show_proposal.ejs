<%- include('../header'); -%>
<%- include('../sidebar'); -%>

<!-- Page Content  -->
<div class="app-main__inner">
   <div class="app-page-title">
       <div class="page-title-wrapper">
           <div class="page-title-heading">
               <div class="page-title-icon">
                   <i class="pe-7s-car icon-gradient bg-mean-fruit"></i>
               </div>
               <div>
                Show Proposal -  <%= proposal.title %>
               </div>
           </div> 
        </div>
   </div>        
   <div class="tabs-animation">
    <div class="row">
        <div class="col-md-12">
            <div class="main-card mb-3 card">
                <div class="table-responsive">
                    <table class="align-middle mb-0 table table-borderless table-striped table-hover">
                            <tr>
                                <th class="">Title</th>
                                <td class=""><%= proposal.title %></td>
                            </tr>
                            <tr>
                                <th class="">Genre</th>
                                <td class=""><%= proposal.genre %></td>
                            </tr>
                            <tr>
                                <th class="">Supplier name</th>
                                <td class=""><%= proposal.supplierName %></td>
                            </tr>
                            <tr>
                                <th class="">Supplier phone no</th>
                                <td class=""><%= proposal.phoneNumber %></td>
                            </tr>
                            <tr>
                                <th class="">Date added</th> 
                                <td class=""><%= moment(proposal.date).format('LL') %></td>
                            </tr>
                            <tr>
                                <th class="">Supplier email</th>
                                <td class=""><%= proposal.email %></td>
                            </tr>
                            <tr>
                                <th class="">Contact name</th>
                                <td class=""><%= proposal.contactName %></td>
                            </tr>
                            <tr>
                                <th class="">Location</th>
                                <td class=""><%= proposal.location %></td>
                            </tr>
                            <tr>
                                <th class="">Presentation Url</th>
                                <td class=""><%= proposal.presentationURL %></td>
                            </tr>
                            <tr>
                                <th class="">Show pilot url</th>
                                <td class=""><%= proposal.showReel %></td>
                            </tr>
                            <tr>
                                <th class="">First time production</th>
                                <td class=""><%= proposal.firstTimeProduction %></td>
                            </tr>
                            <tr>
                                <th class="">personal relationships</th>
                                <td class=""><%= proposal.personalRelationships %></td>
                            </tr>
                     
                    </table>
                </div>
            </div>
        </div>
    </div>
    
  </div>
</div>
<!-- Page Content END -->


<%- include('../footer'); -%>
<%- include('../js'); -%>
<%- include('../../end'); -%>