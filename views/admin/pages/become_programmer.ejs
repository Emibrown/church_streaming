<%- include('../header'); -%>
<%- include('../sidebar'); -%>

<!-- Page Content  -->
<div class="app-main__inner">
   <div class="app-page-title">
       <div class="page-title-wrapper">
           <div class="page-title-heading">
               <div class="page-title-icon">
                   <i class="pe-7s-car icon-gradient bg-mean-fruit"></i>
               </div>
               <div>
                Become a programmer - <%= programmer.fullname %>
               </div>
           </div> 
        </div>
   </div>        
   <div class="tabs-animation">
    <div class="row">
        <div class="col-md-12">
            <div class="main-card mb-3 card">
                <div class="table-responsive">
                    <table class="align-middle mb-0 table table-borderless table-striped table-hover">
                            <tr>
                                <th class="">Full name</th>
                                <td class=""><%= programmer.fullName %></td>
                            </tr>
                            <tr>
                                <th class="">email</th>
                                <td class=""><%= programmer.email %></td>
                            </tr>
                            <tr>
                                <th class="">phone no</th>
                                <td class=""><%= programmer.phone %></td>
                            </tr>
                            <tr>
                                <th class="">Date added</th> 
                                <td class=""><%= moment(programmer.date).format('LL') %></td>
                            </tr>
                            <tr>
                                <th class="">Ministry name</th>
                                <td class=""><%= programmer.ministryName %></td>
                            </tr>
                            <tr>
                                <th class="">Website url or facebook page</th>
                                <td class=""><%= programmer.webURL %></td>
                            </tr>
                            <tr>
                                <th class="">Location</th>
                                <td class=""><%= programmer.location %></td>
                            </tr>
                            <tr>
                                <th class="">Video Url</th>
                                <td class=""><%= programmer.videoURL %></td>
                            </tr>
                            <tr>
                                <th class="">Programme title</th>
                                <td class=""><%= programmer.programmeTitle %></td>
                            </tr>
                            <tr>
                                <th class="">ptrogramme description</th>
                                <td class=""><%= programmer.programmeDescription %></td>
                            </tr>
                            <tr>
                                <th class="">First time production</th>
                                <td class=""><%= programmer.firstTimeProduction %></td>
                            </tr>
                            <tr>
                                <th class="">personal relationships</th>
                                <td class=""><%= programmer.personalRelationships %></td>
                            </tr>
                     
                    </table>
                </div>
            </div>
        </div>
    </div>
    
  </div>
</div>
<!-- Page Content END -->


<%- include('../footer'); -%>
<%- include('../js'); -%>
<%- include('../../end'); -%>