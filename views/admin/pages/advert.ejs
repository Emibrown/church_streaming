<%- include('../header'); -%>
<%- include('../sidebar'); -%>

<!-- Page Content  -->
<div class="app-main__inner">
   <div class="app-page-title">
       <div class="page-title-wrapper">
           <div class="page-title-heading">
               <div class="page-title-icon">
                   <i class="pe-7s-car icon-gradient bg-mean-fruit"></i>
               </div>
               <div>
                <%= proposal.title %> Show Proposal
               </div>
           </div> 
        </div>
   </div>        
   <div class="tabs-animation">
    <div class="row">
        <div class="col-md-12">
            <div class="main-card mb-3 card">
                <div class="table-responsive">
                    <table class="align-middle mb-0 table table-borderless table-striped table-hover">
                            <tr>
                                <th class="text-center">Title</th>
                                <td class="text-center"><%= advert.title %></td>
                            </tr>
                            <tr>
                                <th class="text-center">Genre</th>
                                <td class="text-center"><%= advert.genre %></td>
                            </tr>
                            <tr>
                                <th class="text-center">Supplier name</th>
                                <td class="text-center"><%= advert.supplierName %></td>
                            </tr>
                            <tr>
                                <th class="text-center">Supplier phone no</th>
                                <td class="text-center"><%= advert.phoneNumber %></td>
                            </tr>
                            <tr>
                                <th class="text-center">Date added</th> 
                                <td class="text-center"><%= moment(advert.date).format('LL') %></td>
                            </tr>
                            <tr>
                                <th class="text-center">Supplier email</th>
                                <td class="text-center"><%= advert.email %></td>
                            </tr>
                            <tr>
                                <th class="text-center">Contact name</th>
                                <td class="text-center"><%= advert.contactName %></td>
                            </tr>
                            <tr>
                                <th class="text-center">Location</th>
                                <td class="text-center"><%= advert.location %></td>
                            </tr>
                            <tr>
                                <th class="text-center">Presentation Url</th>
                                <td class="text-center"><%= advert.presentationURL %></td>
                            </tr>
                            <tr>
                                <th class="text-center">Show pilot url</th>
                                <td class="text-center"><%= advert.showReel %></td>
                            </tr>
                            <tr>
                                <th class="text-center">First time production</th>
                                <td class="text-center"><%= advert.firstTimeProduction %></td>
                            </tr>
                            <tr>
                                <th class="text-center">personal relationships</th>
                                <td class="text-center"><%= advert.personalRelationships %></td>
                            </tr>
                     
                    </table>
                </div>
            </div>
        </div>
    </div>
    
  </div>
</div>
<!-- Page Content END -->


<%- include('../footer'); -%>
<%- include('../js'); -%>
<%- include('../../end'); -%>